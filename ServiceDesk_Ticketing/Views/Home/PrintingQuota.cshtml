@{
    ViewData["Title"] = "Printing Quota";
}
<!-- IT Service/Support Request Session Section -->


<div class="container mt-5">
    <div class="text-center mb-4">
        <h3>IT Service/Support Request Session</h3>
    </div>
    
    <div class="text-center mb-4">
        <h4>Printing Quota</h4>
    </div>

    <form>
        <div class="mb-3">
            <label for="icNum" class="form-label">IC num:</label>
            <input type="text" id="icNum" name="icNum" class="form-control" placeholder="Type here...">
        </div>
        <div class="mb-3">
            <label for="increaseQty" class="form-label">Increase Qty:</label>
            <input type="text" id="increaseQty" name="increaseQty" class="form-control" placeholder="Type here...">
        </div>
        <div class="mb-3">
            <label for="reasonForIncrease" class="form-label">Reason for increase:</label>
            <input type="text" id="reasonForIncrease" name="reasonForIncrease" class="form-control" placeholder="Type here...">
        </div>
        <button type="submit" class="btn btn-primary w-100">Submit</button>
    </form>
    <div class="text-center mt-4">
        <a href="@Url.Action("Index", "Home")" style="display: inline-block; margin-top: 20px;">Back to Create Ticket</a>
    </div>
</div>
